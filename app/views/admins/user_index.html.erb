<div class="container center">
    <h1 class="admin_title">会員一覧</h1>
    <table class="table">
        <thead>
            <th class="center">id</th><th class="center">名前</th><th class="center">メールアドレス</th><th class="center">性別</th><th class="center">ポイント</th>
        </thead>
        <tbody>
            <% @users.each do |user| %>
                <tr>
                    <td><%= user.id %></td>
                    <td><%= user.name %></td>
                    <td><%= user.email %></td>
                    <td><%= user.sex %></td>
                    <td><%= user.point %></td>
                </tr>
            <% end %>
        </tbody>
    </table>
</div>